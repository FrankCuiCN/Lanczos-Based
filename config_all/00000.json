  "epoch_num": 100,
  "batch_size_train": 512,
  "batch_size_test": 512,
  "lr_schedule": [[0, 0.001], [49, 0.001], [50, 0.0001], [69, 0.0001], [70, 1e-05], [89, 1e-05], [90, 1e-06], [99, 1e-06]],
  "rp_schedule": [[0, 0.25], [49, 0.25], [50, 0.5], [69, 0.5], [70, 0.75], [89, 0.75], [90, 0.95], [99, 0.95]],
  "lz_schedule": [[0, 2], [49, 2], [50, 4], [69, 4], [70, 8], [89, 8], [90, 16], [99, 16]],
  "test_adv_milestones": [90, 91, 92, 93, 94, 95, 96, 97, 98, 99]}