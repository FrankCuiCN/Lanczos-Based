{
  "dataset": "cifar10",
  "train_mode": "jacobian",
  "reg_mode": "power_method",
  "hutchinson_factor": 1,
  "torch_allow_tf32": true,
  "torch_benchmark": true,
  "model": "resnet",
  "activation": "softplus",
  "sp_beta": 8,
  "sp_thr": 20,
  "convolution": "std_conv",
  "normalization": "group_norm",
  "gn_mode": 0,
  "gn_num_groups": 32,
  "gn_channels_per_group": 4,
  "optimizer": "adam",
  "weight_decay": 0.0005,
  "sgd_momentum": 0.9,
  "sgd_nesterov": true,
  "adamw_eps": 1e-08,
